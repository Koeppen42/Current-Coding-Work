function main(){
   
moveRight();
moveUpRight();
moveLeft();
moveUpLeft();
moveRight();
moveUpRight();
moveLeft();
moveUpLeft();
moveRight();


}

function moveRight() {
   putBeeper();
   move();
   move();
   putBeeper();
   move();
   move();
   putBeeper();
   
}

function moveUpRight() {
   turnLeft();
   move();
   turnLeft();
}

function moveLeft() {
   move();
   putBeeper();
   move();
   move();
   putBeeper();
   move();
}

function moveUpLeft() {
   turnRight();
   move();
   turnRight();
}
